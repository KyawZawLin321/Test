  <artifactId>StudentRegistrationJPA</artifactId>
  <version>1.0</version>
  <packaging>war</packaging>
   <build>
    <plugins>
        <!-- Compiler plugin -->
        <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-compiler-plugin</artifactId>
            <version>3.8.1</version> <!-- Or the version you are using -->
            <configuration>
                <source>17</source> <!-- Or your Java version -->
                <target>17</target> <!-- Or your Java version -->
            </configuration>
        </plugin>
        <!-- War plugin -->
        <plugin>
            <groupId>org.apache.maven.plugins</groupId>
            <artifactId>maven-war-plugin</artifactId>
            <version>3.3.1</version> <!-- Or the version you are using -->
            <configuration>
                <!-- Specify the web.xml location -->
                <failOnMissingWebXml>false</failOnMissingWebXml>
            </configuration>
        </plugin>
    </plugins>
   
    
</build>
